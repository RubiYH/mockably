{
  "name": "mockably",
  "version": "0.0.0",
  "description": "Realtime Backend Implementation on Frontend - Main entry package",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && rollup -c",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "keywords": [
    "frontend",
    "backend",
    "database",
    "realtime",
    "indexeddb",
    "p2p"
  ],
  "author": "RubiYH <contact@yoonho.dev>",
  "license": "MIT",
  "dependencies": {
    "@mockably/core": "0.0.0",
    "@mockably/utils": "0.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "eslint": "^9.30.1",
    "rollup": "^4.21.2",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
