{
  "name": "@mockably/utils",
  "version": "0.0.0",
  "description": "Utility functions for Mockably ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && rollup -c",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist",
    "test": "vitest"
  },
  "keywords": [
    "utils",
    "utilities",
    "mockably"
  ],
  "author": "RubiYH <contact@yoonho.dev>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "eslint": "^9.30.1",
    "rollup": "^4.21.2",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  }
}
